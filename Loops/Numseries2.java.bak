class Numseries2 
{
	public static void main(String[] args) 
	{
		
        int terms = 6;  // Number of terms in the series
        long prevTerm = 1;  // Start with the first term (1)
        
        // Print the first term
        System.out.print(prevTerm + " ");
        
        // Loop to generate the terms of the series
        for (int i = 1; i < terms; i++) {
            if (i == 2) {
                prevTerm = prevTerm * 3 + 6;  // Apply multiplication by 3 + 6
            } else if (i == 3) {
                prevTerm = prevTerm * 5;  // Apply multiplication by 5
            } else if (i == 4) {
                prevTerm = prevTerm * 6;  // Apply multiplication by 6
            } else {
                prevTerm = prevTerm * 3;  // Apply multiplication by 3 (for other cases)
            }
            System.out.print(prevTerm + " ");  // Print the current term
        }





